{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "hsl",
      "camelcase": "Hsl",
      "title": "HyperSpace Lang",
      "scope": "source.hsl",
      "file-types": [
        "hsl"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^hsl$",
      "class-name": "TreeSitterHsl"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "The HyperSpace Lang tree sitter grammar",
    "authors": [
      {
        "name": "Thibault Cheneviere",
        "email": "thibault.chene23@gmail.com",
        "url": "https://thibault-cne.fr/"
      }
    ],
    "links": {
      "repository": "https://github.com/hs-lang/tree-sitter-hsl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
